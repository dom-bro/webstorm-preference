<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="autoExpressions" value="false" />
      <show-values-inline>false</show-values-inline>
    </data-views>
    <general>
      <run-to-cursor-gesture>false</run-to-cursor-gesture>
      <option name="showDebuggerOnBreakpoint" value="false" />
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration alwaysDoSmartStepInto="false" doNotStepIntoLibraryCode="false">
          <custom-object-presentation />
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>